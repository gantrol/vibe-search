{
    "name": "vibe-search",
    "version": "0.1.1",
    "description": "LLM-vibe search over your text content using Google Gemini (ESM)",
    "type": "module",
    "main": "src/index.js",
    "module": "src/index.js",
    "types": "src/index.d.ts",
    "exports": {
        ".": {
            "types": "./src/index.d.ts",
            "import": "./src/index.js"
        }
    },
    "files": [
        "src/",
        "README.md",
        "README.zh-CN.md",
        "LICENSE"
    ],
    "scripts": {
        "start": "node examples/run.js",
        "eval": "node examples/evaluate.js",
        "test": "node examples/smoke.mjs",
        "test:install": "node examples/install-test.js",
        "test:react": "node examples/test-react-example.js",
        "check": "node scripts/pre-release-check.js",
        "lint": "node -e \"console.log('No linter configured')\"",
    "prepublishOnly": "npm run check && npm test"
    },
    "keywords": [
        "gemini",
        "google",
        "genai",
        "search",
        "llm"
    ],
    "author": "gantrol",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gantrol/vibe-search-gemini.git"
    },
    "bugs": {
        "url": "https://github.com/gantrol/vibe-search-gemini/issues"
    },
    "homepage": "https://github.com/gantrol/vibe-search-gemini#readme",
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@google/genai": "^0.3.0",
        "dotenv": "^16.4.5"
    },
    "peerDependencies": {},
    "devDependencies": {}
}